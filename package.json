    "chalk": "^4.1.2",
    "country-flags-svg": "1.1.7",
    "ejs": "^3.1.8",
    "express": "^4.17.1",
    "jimp": "^0.22.4",
    "log4js": "^6.3.0",
    "micromatch": "^4.0.4",
    "modclean": "^3.0.0-beta.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-aes-cmac": "^0.1.1",
    "p-limit": "^4.0.0",
    "parse-duration": "^1.0.2",
    "sequelize": "^6.7.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",

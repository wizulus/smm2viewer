{
  "name": "app",
  "version": "0.0.1",
  "description": "Super Mario Maker Level Viewer",
  "productName": "SMM2 Level Viewer",
  "author": "Wizulus Redikulus (wizulus@gmail.com)",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "server:dev": "nodemon --config src-server/nodemon.json src-server/main.js",
    "client:dev": "quasar dev --mode pwa",
    "dev": "npm-run-all -p server:dev client:dev",
    "build:clean": "rm -rf dist/",
    "build:server": "rollup -c",
    "build:client": "quasar build --mode pwa",
    "build:icons": "icongenie generate --skip-trim -i src/assets/icon.png",
    "build": "npm-run-all build:clean build:icons build:client build:server",
    "help": "quasar --help"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.46.0",
